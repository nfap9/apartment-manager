NODE_ENV=development
PORT=3000

# Postgres (set by docker-compose in later steps)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/apartment_manager?schema=public

# Redis (BullMQ)
REDIS_URL=redis://localhost:6379

# JWT secrets (use strong random strings in production)
JWT_ACCESS_SECRET=please_change_me_access_secret_32_chars_min
JWT_REFRESH_SECRET=please_change_me_refresh_secret_32_chars_min

# CORS (comma separated). Leave empty to allow Vite default in dev.
CORS_ORIGINS=http://localhost:5173

